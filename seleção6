package main

import "fmt"

func main() {
	var num1 int
	var num2 int

	fmt.Print("Insira o primeiro número inteiro: ")
	fmt.Scan(&num1)
	fmt.Print("Insira o segundo número inteiro: ")
	fmt.Scan(&num2)

	if num1 >= 0 && num2 >= 0 {
		fmt.Print("O resultado da multiplicação entre os dois números é: ", num1*num2)
	} else if num1 < 0 || num2 < 0 {
		fmt.Print("O resultado da soma entre os dois números é: ", num1+num2)
	}
}
