package main

import "fmt"

func main() {
	var num1 int
	var num2 int

	fmt.Print("Insira o primeiro número inteiro: ")
	fmt.Scan(&num1)
	fmt.Print("Insira o segundo número inteiro: ")
	fmt.Scan(&num2)

	if num1 < num2 {
		fmt.Println("O maior número é", num2)
	} else if num1 == num2 {
		fmt.Print("Os dois números são iguais!")
	} else if num1 > num2 {
		fmt.Println("O maior número é:", num1)
	}
}
