package main

import "fmt"

func main() {
	var num1 float64
	var num2 float64
	var num3 float64

	fmt.Print("Insira o seu primeiro número: ")
	fmt.Scan(&num1)
	fmt.Print("Insira o seu segundo número: ")
	fmt.Scan(&num2)
	fmt.Print("Insira o seu terceiro número: ")
	fmt.Scan(&num3)

	var mediaponderada float64 = ((num1 * 2) + (num2 * 3) + (num3 * 5)) / 10

	fmt.Println("A média ponderada é", mediaponderada)
}
