package main

import "fmt"

func main() {
	var idade int

	fmt.Print("Insira sua idade: ")
	fmt.Scan(&idade)

	if idade <= 9 {
		fmt.Print("Sua classificação: Mirim.")
	} else if idade >= 10 && idade <= 13 {
		fmt.Print("Sua classificação: Infantil.")
	} else if idade >= 14 && idade <= 17 {
		fmt.Print("Sua classificação: Juvenil.")
	} else if idade >= 18 {
		fmt.Print("Sua classificação: Adulto.")
	}
}
