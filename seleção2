package main

import "fmt"

func main() {
	var num1 int
	var num2 int
	var num3 int

	fmt.Print("Insira o primeiro número inteiro:")
	fmt.Scan(&num1)
	fmt.Print("Insira o segundo número inteiro:")
	fmt.Scan(&num2)
	fmt.Print("Insira o terceiro número inteiro:")
	fmt.Scan(&num3)

	if num1 < num2 && num1 < num3 {
		fmt.Println("O menor número é", num1)
	} else if (num1 == num2) && (num1 == num3) {
		fmt.Println("Os números são iguais!")
	} else if num2 < num1 && num2 < num3 {
		fmt.Println("O menor número é", num2)
	} else if num3 < num1 && num3 < num2 {
		fmt.Println("O menor número é", num3)
	}

}

